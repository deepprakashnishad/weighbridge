{
  "name": "weighbridge",
  "version": "0.0.0",
  "author": "Deep Prakash Nishad",
  "description": "Weighbridge by Notamedia",
  "main": "electron-main.js",
  "build": {
    "appId": "com.notamedia.weighbridge",
    "productName": "Notamedia Weighbridge",
    "buildVersion":  "0.0.0",
    "files": [ "**/*", "print.py" ],
    "directories": {
      "output": "release",
      "buildResources": "app"
    },
    "asar": true,
    "win": {
      "target": [
        "nsis",
        "nsis-web",
        "portable"
      ]
    },
    "nsis": {
      "runAfterFinish": false
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --live-reload",
    "build": "ng build --prod --base-href ./",
    "rebuild": "electron-rebuild",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dist": "electron-builder",
    "electron-start": "electron ./app/electron-main.js",
    "package-win": "electron-packager ./app wb --overwrite --ignore=\"(e2e/.*|src/.*|.browserslistrc|.editorconfig|.gitignore|angular.json|karma.conf.js|package-lock.json|README.md|tsconfig.app.json|tsconfig.json|tsconfig.spec.json|tslint.json)\" --asar=false --platform=win32 --arch=ia32 --icon=./dist/angular-build/assets/icons8-connect-256.ico --prune=true --out=./dist --version-string.CompanyName=Notamedia --version-string.FileDescription=Notamedia --version-string.ProductName=\"Weighbridge\"",
    "build-mac": "ng build --prod --base-href ./ && electron-packager . --overwrite --ignore=\"(e2e/.*|node_modules/.*|src/.*|.browserslistrc|.editorconfig|.gitignore|angular.json|karma.conf.js|package-lock.json|README.md|tsconfig.app.json|tsconfig.json|tsconfig.spec.json|tslint.json)\" --platform=darwin --arch=x64 --icon=./dist/angular-build/assets/icons8-connect-256.icns --prune=true --out=./dist/electron-build",
    "build-win": "ng build --prod --base-href ./ && electron-packager . weighbridge --overwrite --ignore=\"(e2e/.*|node_modules/.*|src/.*|.browserslistrc|.editorconfig|.gitignore|angular.json|karma.conf.js|package-lock.json|README.md|tsconfig.app.json|tsconfig.json|tsconfig.spec.json|tslint.json)\" --asar --platform=win32 --arch=ia32 --icon=./dist/angular-build/assets/icons8-connect-256.ico --prune=true --out=./dist/electron-build --version-string.CompanyName=Notamedia --version-string.FileDescription=Notamedia --version-string.ProductName=\"Weighbridge\"",
    "build-linux": "ng build --prod --base-href ./ && electron-packager . demo-app --overwrite --ignore=\"(e2e/.*|node_modules/.*|src/.*|.browserslistrc|.editorconfig|.gitignore|angular.json|karma.conf.js|package-lock.json|README.md|tsconfig.app.json|tsconfig.json|tsconfig.spec.json|tslint.json)\" --asar --platform=linux --arch=x64 --icon=./dist/angular-build/assets/icons8-connect-256.png --prune=true --out=./dist/electron-build",
    "build-winux": "ng build --prod --base-href ./ && electron-packager . demo-app --overwrite --ignore=\"(e2e/.*|node_modules/.*|src/.*|.browserslistrc|.editorconfig|.gitignore|angular.json|karma.conf.js|package-lock.json|README.md|tsconfig.app.json|tsconfig.json|tsconfig.spec.json|tslint.json)\" --asar --platform=win32 --arch=ia32 --icon=./dist/angular-build/assets/icons8-connect-256.ico --prune=true --out=./dist/electron-build --version-string.CompanyName=SciTechEnthusiast --version-string.FileDescription=SciTechEnthusiast --version-string.ProductName=\"Demo Angular Electron app\" && electron-packager . demo-app --overwrite --ignore=(\"e2e|node_modules|src|.browserslistrc|.editorconfig|.gitignore|angular.json|karma.conf.js|package-lock.json|README.md|tsconfig.app.json|tsconfig.json|tsconfig.spec.json|tslint.json\") --asar --platform=linux --arch=x64 --icon=./dist/angular-build/assets/icons8-connect-256.png --prune=true --out=./dist/electron-build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.5",
    "@angular/cdk": "^11.2.4",
    "@angular/common": "^11.2.5",
    "@angular/compiler": "^11.2.5",
    "@angular/core": "^11.2.5",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^11.2.5",
    "@angular/material": "^11.2.4",
    "@angular/platform-browser": "^11.2.5",
    "@angular/platform-browser-dynamic": "^11.2.5",
    "@angular/router": "^11.2.5",
    "angular-notifier": "^9.0.1",
    "core-js": "^2.5.4",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "ngx-color-picker": "^11.0.0",
    "ngx-electron": "^2.2.0",
    "ngx-json-viewer": "^3.0.2",
    "ngx-material-timepicker": "^5.5.3",
    "rxjs": "^6.6.6",
    "rxjs-compat": "^6.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.4",
    "@angular/cli": "^11.2.4",
    "@angular/compiler-cli": "^11.2.5",
    "@angular/language-service": "^11.2.5",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^16.0.0",
    "codelyzer": "^6.0.0",
    "electron": "^13.1.6",
    "electron-builder": "^22.11.7",
    "electron-packager": "^15.2.0",
    "electron-rebuild": "^2.3.5",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
}
